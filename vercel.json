{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "functions": {
    "api/index.py": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/simple.py": {
      "memory": 512,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/",
      "destination": "/api/index.py"
    }
  ]
}
